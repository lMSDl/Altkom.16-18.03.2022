{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HealthChecks-UI": {
    "HealthChecks": [
      {
        "Name": "Web",
        "Uri": "http://localhost:5000/health"
      }
    ],

    "EvalutationTimeOnSeconds": 1,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
